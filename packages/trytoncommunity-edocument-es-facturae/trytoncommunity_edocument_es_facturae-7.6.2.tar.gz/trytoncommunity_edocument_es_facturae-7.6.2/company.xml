<tryton>
    <data>

        <record model="ir.ui.view" id="company_form_view">
            <field name="model">company.company</field>
            <field name="inherit" ref="company.company_view_form"/>
            <field name="name">company_form</field>
        </record>
        <record model="ir.model.field.access" id="access_company_es_facturae_private_key">
            <field name="model">company.company</field>
            <field name="field">es_facturae_private_key</field>
            <field name="perm_read" eval="False"/>
            <field name="perm_write" eval="False"/>
        </record>
        <record model="ir.model.field.access" id="access_company_es_facturae_private_key_account">
            <field name="model">company.company</field>
            <field name="field">es_facturae_private_key</field>
            <field name="group" ref="account.group_account_admin"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="True"/>
        </record>

        <record model="ir.model.field.access" id="access_company_es_facturae_certificate">
            <field name="model">company.company</field>
            <field name="field">es_facturae_certificate</field>
            <field name="perm_read" eval="False"/>
            <field name="perm_write" eval="False"/>
        </record>
        <record model="ir.model.field.access" id="access_company_es_facturae_certificate_account">
            <field name="model">company.company</field>
            <field name="field">es_facturae_certificate</field>
            <field name="group" ref="account.group_account_admin"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="True"/>
        </record>

    </data>
</tryton>
