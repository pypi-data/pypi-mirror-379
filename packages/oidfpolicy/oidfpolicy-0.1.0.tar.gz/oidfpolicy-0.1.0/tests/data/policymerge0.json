{
  "openid_relying_party": {
    "contacts": {
      "add": [
        "helpdesk@federation.example.org",
        "helpdesk@org.example.org"
      ]
    },
    "grant_types": {
      "default": [
        "authorization_code"
      ],
      "subset_of": [
        "authorization_code"
      ],
      "superset_of": [
        "authorization_code"
      ]
    },
    "subject_type": {
      "value": "pairwise"
    },
    "token_endpoint_auth_method": {
      "essential": true,
      "one_of": [
        "self_signed_tls_client_auth"
      ]
    },
    "token_endpoint_auth_signing_alg": {
      "one_of": [
        "PS256",
        "ES256"
      ]
    }
  }
}
