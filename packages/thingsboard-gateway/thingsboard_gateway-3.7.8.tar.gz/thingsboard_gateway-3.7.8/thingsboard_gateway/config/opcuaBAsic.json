{
  "logLevel": "DEBUG",
  "name": "OPCUA BAsic",
  "enableRemoteLogging": false,
  "id": "d2d65828-b033-4a34-b053-d2167a0de75e",
  "configVersion": "3.6.1",
  "server": {
    "url": "opc.tcp://192.168.1.202:4840/freeopcua/server/",
    "timeoutInMillis": 1000,
    "scanPeriodInMillis": 3600000,
    "pollPeriodInMillis": 5000,
    "enableSubscriptions": true,
    "subCheckPeriodInMillis": 100,
    "showMap": false,
    "security": "Basic128Rsa15",
    "identity": {
      "type": "anonymous"
    }
  },
  "mapping": [
    {
      "deviceNodeSource": "path",
      "deviceNodePattern": "Root\\.Objects\\.MyObject",
      "deviceInfo": {
        "deviceNameExpression": "OPCUA Basic Device",
        "deviceNameExpressionSource": "constant",
        "deviceProfileExpressionSource": "constant",
        "deviceProfileExpression": "default"
      },
      "attributes": [
        {
          "key": "Power",
          "type": "string",
          "value": "${Power}"
        },
        {
          "key": "Frequency",
          "type": "string",
          "value": "${Frequency}"
        }
      ],
      "timeseries": [
        {
          "key": "Temperature",
          "type": "string",
          "value": "${Temperature}"
        },
        {
          "key": "Humidity",
          "type": "string",
          "value": "${Humidity}"
        }
      ],
      "rpc_methods": [],
      "attributes_updates": []
    }
  ]
}