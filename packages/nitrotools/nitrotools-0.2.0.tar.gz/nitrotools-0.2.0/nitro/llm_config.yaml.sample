servers:
  llamacpp:
    - endpoint: "http://10.2.0.5:11435"
      interface: "openai_compatible"
      models:
        - name: "qwen"
          model_name: "gpt-oss-20b-Q6_K.gguf"
          temperature: 0.7
          max_tokens: 1000
        - name: "deepseek"
          model_name: "deepseek-coder.gguf"
          temperature: 0.1
          max_tokens: 1500
  openrouter:
    endpoint: "https://openrouter.ai/api/v1"
    api_key: "${OPENROUTER_API_KEY}"
    interface: "openai_compatible"
    headers:
      HTTP-Referer: "${OPENROUTER_HTTP_REFERER}"
      X-Title: "${OPENROUTER_X_TITLE}"
    models:
      - name: "grok-fast-free"
        model: "x-ai/grok-4-fast:free"
        temperature: 0.3
        max_tokens: 1000
      - name: "nemotron-nano-free"
        model: "nvidia/nemotron-nano-9b-v2:free"
        temperature: 0.1
        max_tokens: 1500

purposes:
  general: "llamacpp:qwen"
  coding: "llamacpp:deepseek"
  reasoning: "openrouter:grok-fast-free"
  analysis: "openrouter:nemotron-nano-free"
  assistant: "openrouter:grok-fast-free"
  route: "llamacpp:qwen"
  retrieval: "llamacpp:qwen"
  assistant_coding: "llamacpp:deepseek"
  assistant_reasoning: "openrouter:grok-fast-free"
