{
  "devtools": {
    "type": "svelte",
    "version": "5",
    "features": {
      "componentInspection": true,
      "storeInspection": true,
      "performanceMonitoring": true,
      "runtimeDebugging": true
    },
    "extensions": [
      {
        "name": "Svelte DevTools",
        "id": "svelte-devtools",
        "enabled": true
      }
    ],
    "sourceMap": {
      "enabled": true,
      "sourceRoot": "/src"
    },
    "project": {
      "name": "TAI Chat",
      "description": "Framework de chat con IA para desarrolladores",
      "framework": "SvelteKit",
      "version": "5.0.0"
    }
  }
}
