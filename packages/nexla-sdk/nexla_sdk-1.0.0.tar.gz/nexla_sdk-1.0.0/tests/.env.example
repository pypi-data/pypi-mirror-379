# Nexla SDK Integration Test Configuration
# Copy this file to .env and fill in your actual values

# Required: Nexla API URL
NEXLA_TEST_API_URL=https://dataops.nexla.io/nexla-api

# Required: Authentication (choose one)
# Option 1: Service Key (recommended)
NEXLA_TEST_SERVICE_KEY=your-service-key-here

# Option 2: Access Token
# NEXLA_TEST_ACCESS_TOKEN=your-access-token-here

# Optional: API Version (default: v1)
NEXLA_TEST_API_VERSION=v1

# Optional: Log Level (default: INFO)
NEXLA_TEST_LOG_LEVEL=INFO
