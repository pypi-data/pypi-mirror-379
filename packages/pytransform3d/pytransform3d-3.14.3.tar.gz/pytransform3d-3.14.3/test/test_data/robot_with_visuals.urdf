      <material name="Orange"/>
    </visual>
    <visual>
      <origin xyz="0 0.14 0" rpy="1.570796 0 0"/>
      <geometry>
        <cylinder radius="0.05" length="0.28"/>
      </geometry>
      <material name="Black"/>
    </visual>
  </link>
  <link name="link3">
    <visual>
      <origin xyz="0 0 0"/>
      <geometry>
        <cylinder radius="0.05" length="0.1"/>
      </geometry>
      <material name="Orange"/>
    </visual>
  </link>
  <link name="link4">
    <visual>
      <origin xyz="0 0 0.17"/>
      <geometry>
        <cylinder radius="0.05" length="0.34"/>
