      "url": "$CONTEXT_REPO/6_4_2_propositus.csv"
    },
    "familyConsanguinity": {
      "category": "concept-map",
      "url": "$CONTEXT_REPO/6_4_4_consanguinity.csv"
    },
    "familyDeceased": {
      "category": "concept-map",
      "url": "$CONTEXT_REPO/6_4_10_family_member_deceased.csv"
    },
    "familyRelationshipPropositus": {
      "category": "concept-map",
