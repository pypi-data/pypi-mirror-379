# 开发环境配置文件示例
# 复制此文件为 .env 并根据需要修改配置

# 应用配置
PLUMELOG_APP_NAME=my_application
PLUMELOG_ENV=development

# Redis配置
PLUMELOG_REDIS_HOST=localhost
PLUMELOG_REDIS_PORT=6379
PLUMELOG_REDIS_DB=0
PLUMELOG_REDIS_PASSWORD=
PLUMELOG_REDIS_KEY=plume_log_list

# 批处理配置
PLUMELOG_BATCH_SIZE=100
PLUMELOG_BATCH_INTERVAL_SECONDS=2.0
PLUMELOG_QUEUE_MAX_SIZE=10000

# 连接配置
PLUMELOG_MAX_CONNECTIONS=5
PLUMELOG_RETRY_COUNT=3
PLUMELOG_RETRY_DELAY=1.0

# 超时配置
PLUMELOG_SOCKET_CONNECT_TIMEOUT=5.0
PLUMELOG_SOCKET_TIMEOUT=5.0

# 缓存配置
PLUMELOG_TEMP_BUFFER_MAX_SIZE=1000
