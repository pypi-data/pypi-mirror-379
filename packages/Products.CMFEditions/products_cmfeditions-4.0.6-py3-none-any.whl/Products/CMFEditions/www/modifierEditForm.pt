<p tal:replace="structure here/manage_page_header"
   omit-tag=""
>Header</p>
<p tal:replace="structure here/manage_tabs"
   omit-tag=""
>tabs</p>


<h2>Edit Modifier</h2>

<form action="edit"
      method="post"
>

  <table border="0">

    <tr>
      <th>
        Title
      </th>
      <td>
        <input name="title"
               size="40"
               type="text"
               value=""
               tal:attributes="
                 value here/title;
               "
        />
      </td>
    </tr>

    <tr>
      <th>
        Enabled
      </th>
      <td>
        <input name="enabled"
               type="checkbox"
               value="True"
               tal:attributes="
                 checked here/isEnabled;
               "
        />
      </td>
    </tr>

    <tr>
      <td colspan="2">
        <input name="submit"
               type="submit"
               value="Save"
        />
      </td>
    </tr>

  </table>

</form>

<p tal:replace="structure here/manage_page_footer"
   omit-tag=""
>Footer</p>
