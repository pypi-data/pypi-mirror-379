{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "App SDK Template Install Example Schema",
  "type": "object",
  "properties": {
    "outbound_proxy": {
      "type": "string",
      "description": "The outbound proxy to be used.",
      "minLength": 1
    },
    "qualify_frequency": {
      "type": "integer",
      "description": "Frequency (in seconds) to qualify contacts.",
      "minLength": 1
    },
    "headers": {
      "type": "array",
      "description": "Headers configuration",
      "items": {
        "type": "object",
        "properties": {
          "header": {
            "type": "string",
            "description": "Header name"
          },
          "value": {
            "type": "string",
            "description": "Header value"
          }
        },
        "required": [
          "header",
          "value"
        ]
      }
    }
  },
  "required": [
    "outbound_proxy",
    "qualify_frequency",
    "headers"
  ]
}