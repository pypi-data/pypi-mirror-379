[setup]
	version = '2.1.0'
	
	[[setup.hooks]]
		expression = 'false'
		location = 'udp.payload'
		protocol = '#RTCP'

[RTCP]
	name = 'Real-Time Transport Control Protocol'
	short_name = 'RTCP'
	tag = 'rtcp'

	[[RTCP.fields]]
		name = 'Version'
		short_name = 'Version'
		tag = 'version'
		length = 2
		format = 'decimal'

	[[RTCP.fields]]
		name = 'Padding'
		short_name = 'Padding'
		tag = 'padding'
		length = 1
		format = 'decimal'

	[[RTCP.fields]]
		name = 'Source count'
		short_name = 'Source count'
		tag = 'sc'
		length = 5
		format = 'decimal'

	[[RTCP.fields]]
		name = 'Packet type'
		short_name = 'Packet type'
		tag = 'pt'
		length = 8
		format = 'decimal'

		[[RTCP.fields.states]]
			value = 192
			name = "Full intra-frame request (H.261)"
	
		[[RTCP.fields.states]]
			value = 193
			name = "Negative acknowledgement (H.261)"
	
		[[RTCP.fields.states]]
			value = 194
			name = "SMPTE time-code mapping"
	
		[[RTCP.fields.states]]
			value = 195
			name = "Extended inter-arrival jitter report"

		[[RTCP.fields.states]]
			value = 200
			name = "Sender report"
	
		[[RTCP.fields.states]]
			value = 201
			name = "Receiver report"
	
		[[RTCP.fields.states]]
			value = 202
			name = "Source description"
	
		[[RTCP.fields.states]]
			value = 203
			name = "Goodbye"
	
		[[RTCP.fields.states]]
			value = 204
			name = "Application specific"
	
		[[RTCP.fields.states]]
			value = 205
			name = "Generic RTP feedback"
	
		[[RTCP.fields.states]]
			value = 206
			name = "Payload-specific feedback"
	
		[[RTCP.fields.states]]
			value = 207
			name = "Extended report (RFC 3611)"
	
		[[RTCP.fields.states]]
			value = 208
			name = "AVB RTCP packet (IEEE 1733)"
	
		[[RTCP.fields.states]]
			value = 209
			name = "Receiver summary information"
	
		[[RTCP.fields.states]]
			value = 210
			name = "Port mapping"
	
	[[RTCP.fields]]
		name = 'Length'
		short_name = 'Length'
		tag = 'length'
		length = 16
		format = 'decimal'