[setup]
	version = '2.1.0'

	[[setup.hooks]]
		expression = 'false'
		location = 'udp.payload'
		protocol = '#RTP'

[RTP]
	name = 'Real-Time Transport Protocol'
	short_name = 'RTP'
	tag = 'rtp'

	[[RTP.fields]]
		name = 'Version'
		short_name = 'Version'
		tag = 'version'
		length = 2
		format = 'decimal'

	[[RTP.fields]]
		name = 'Padding'
		short_name = 'Padding'
		tag = 'padding'
		length = 1

	[[RTP.fields]]
		name = 'Extension'
		short_name = 'Extension'
		tag = 'ext'
		length = 1

	[[RTP.fields]]
		name = 'Contributing source identifiers count'
		tag = 'cc'
		short_name = 'CSRC'
		length = 4

	[[RTP.fields]]
		name = 'Marker'
		short_name = 'Marker'
		tag = 'marker'
		length = 1

	[[RTP.fields]]
		name = 'Payload type'
		short_name = 'Payload type'
		tag = 'p_type'
		length = 7

		[[RTP.fields.states]]
			value = 0
			name = "ITU-T G.711 PCMU"
	
		[[RTP.fields.states]]
			value = 1
			name = "USA Federal Standard FS-1016"
	
		[[RTP.fields.states]]
			value = 2
			name = "ITU-T G.721"
	
		[[RTP.fields.states]]
			value = 3
			name = "GSM 06.10"
	
		[[RTP.fields.states]]
			value = 4
			name = "ITU-T G.723"
	
		[[RTP.fields.states]]
			value = 5
			name = "DVI4 8000"
	
		[[RTP.fields.states]]
			value = 6
			name = "DVI4 16000"
	
		[[RTP.fields.states]]
			value = 7
			name = "LPC"
	
		[[RTP.fields.states]]
			value = 8
			name = "ITU-T G.711 PCMA"
	
		[[RTP.fields.states]]
			value = 9
			name = "ITU-T G.722"
	
		[[RTP.fields.states]]
			value = 10
			name = "16-bit uncompressed stereo audio"
	
		[[RTP.fields.states]]
			value = 11
			name = "16-bit uncompressed mono audio"
	
		[[RTP.fields.states]]
			value = 12
			name = "QCELP"
	
		[[RTP.fields.states]]
			value = 13
			name = "Comfort noise"
	
		[[RTP.fields.states]]
			value = 14
			name = "MPEG-I/II audio"
	
		[[RTP.fields.states]]
			value = 15
			name = "ITU-T G.728"
	
		[[RTP.fields.states]]
			value = 16
			name = "DVI4 11025"
	
		[[RTP.fields.states]]
			value = 17
			name = "DVI4 22050"
	
		[[RTP.fields.states]]
			value = 17
			name = "ITU-T G.729"
	
		[[RTP.fields.states]]
			value = 26
			name = "JPEG-compressed video"
	
		[[RTP.fields.states]]
			value = 31
			name = "ITU-T H.261"
	
		[[RTP.fields.states]]
			value = 32
			name = "MPEG-I/II video"
	
		[[RTP.fields.states]]
			value = 33
			name = "MPEG-II transport"
	
		[[RTP.fields.states]]
			value = 34
			name = "ITU-T H.263"
	
		[[RTP.fields.states]]
			start = 35
			end = 71
			name = "Unassigned"
	
		[[RTP.fields.states]]
			start = 72
			end = 76
			name = "Reserved for RTCP conflict avoidance"
	
		[[RTP.fields.states]]
			start = 77
			end = 95
			name = "Unassigned"
	
		[[RTP.fields.states]]
			start = 96
			end = 127
			name = "Dynamic RTP Type"

	[[RTP.fields]]
		name = 'Sequence Number'
		short_name = 'Seq'
		tag = 'seq'
		length = 16

	[[RTP.fields]]
		name = 'Timestamp'
		short_name = 'Time'
		tag = 'timestamp'
		length = 32

	[[RTP.fields]]
		name = 'Synchronization source identifier'
		short_name = 'SSRC'
		tag = 'ssrc'
		length = 32