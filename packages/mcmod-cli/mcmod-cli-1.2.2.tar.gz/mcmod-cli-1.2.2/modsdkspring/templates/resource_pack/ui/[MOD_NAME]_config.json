{
    "main": {
        "type": "screen",
        "controls": [
            {
                "main_panel@grape_settings.main_panel": {}
            },
            {
                "config_detail_panel@grape_settings.config_detail_panel": {
                    "controls": [
                        {
                            "config_scroll_view@common.scrolling_panel": {
                                "$background_nine_slice_buttom": 0,
                                "$background_nine_slice_left": 0,
                                "$background_nine_slice_right": 0,
                                "$background_nine_slice_top": 0,
                                "$background_nineslice_size": [
                                    0,
                                    0,
                                    0,
                                    0
                                ],
                                "$box_nine_slice_buttom": 0,
                                "$box_nine_slice_left": 0,
                                "$box_nine_slice_right": 0,
                                "$box_nine_slice_top": 0,
                                "$box_nineslice_size": [
                                    0,
                                    0,
                                    0,
                                    0
                                ],
                                "$is_background_nine_slice": false,
                                "$is_box_nine_slice": false,
                                "$is_track_nine_slice": false,
                                "$scroll_background_image_control": "netease_editor_template_namespace.scroll_background_image",
                                "$scroll_background_texture": "textures/ui/ScrollRail",
                                "$scroll_box_mouse_image_control": "netease_editor_template_namespace.scroll_box_image",
                                "$scroll_box_texture": "textures/ui/newTouchScrollBox",
                                "$scroll_box_touch_image_control": "netease_editor_template_namespace.scroll_box_image",
                                "$scroll_track_image_control": "netease_editor_template_namespace.scroll_track_image",
                                "$scroll_track_texture": "textures/ui/ScrollRail",
                                "$scrolling_content": "[MOD_NAME]_config.template_config_detail",
                                "$show_background": true,
                                "$track_nine_slice_buttom": 0,
                                "$track_nine_slice_left": 0,
                                "$track_nine_slice_right": 0,
                                "$track_nine_slice_top": 0,
                                "$track_nineslice_size": [
                                    0,
                                    0,
                                    0,
                                    0
                                ],
                                "anchor_from": "bottom_left",
                                "anchor_to": "bottom_left",
                                "layer": 9,
                                "offset": [
                                    0,
                                    0
                                ],
                                "size": [
                                    "100%",
                                    "100%"
                                ]
                            }
                        }
                    ]
                }
            }
        ]
    },
    "namespace": "[MOD_NAME]_config",
    "template_config_detail": {
        "type": "stack_panel",
        "anchor_from": "bottom_right",
        "anchor_to": "bottom_right",
        "size": [
            "100%+0px",
            "100%+0px"
        ],
        "layer": 1,
        "controls": [
            {
                "toggle_mod_enable_panel": {
                    "type": "stack_panel",
                    "size": [
                        "100%+0px",
                        35.0
                    ],
                    "layer": 75,
                    "orientation": "horizontal",
                    "controls": [
                        {
                            "toggle_mod_enable@common_toggles.switch_toggle_collection": {
                                "type": "panel",
                                "$default_texture": "textures/ui/toggle_off",
                                "$hover_texture": "textures/ui/toggle_on",
                                "$pressed_no_hover_texture": "textures/ui/toggle_on_hover",
                                "$pressed_texture": "textures/ui/toggle_off_hover",
                                "$toggle_name": "toggle_mod_enable",
                                "$toggle_state_binding_name": "#netease_editor_template_namespace.toggle_state",
                                "size": [
                                    "100%y+0px",
                                    "100%+0px"
                                ],
                                "layer": 1,
                                "bindings": [
                                    {
                                        "binding_type": "collection_details",
                                        "binding_collection_name": "toggle_mod_enable",
                                        "binding_condition": "always_when_visible"
                                    }
                                ]
                            }
                        },
                        {
                            "label": {
                                "type": "label",
                                "size": [
                                    "80%+0px",
                                    "100%+0px"
                                ],
                                "layer": 73,
                                "font_type": "default",
                                "line_padding": 22.0,
                                "text": "[MOD_NAME]总开关"
                            }
                        }
                    ]
                }
            }
        ]
    }
}