<odoo>
<record id="partner_evaluation_result_view_tree" model="ir.ui.view">
    <field name="name">partner_evaluation_result - Tree</field>
    <field name="model">partner_evaluation_result</field>
    <field name="mode">primary</field>
    <field name="inherit_id" ref="ssi_master_data_mixin.mixin_master_data_view_tree" />
    <field name="arch" type="xml">
        <data />
    </field>
</record>

<record id="partner_evaluation_result_view_search" model="ir.ui.view">
    <field name="name">partner_evaluation_result - Search</field>
    <field name="model">partner_evaluation_result</field>
    <field name="mode">primary</field>
    <field
            name="inherit_id"
            ref="ssi_master_data_mixin.mixin_master_data_view_search"
        />
    <field name="arch" type="xml">
        <data />
    </field>
</record>

<record id="partner_evaluation_result_view_form" model="ir.ui.view">
    <field name="name">partner_evaluation_result - Form</field>
    <field name="model">partner_evaluation_result</field>
    <field name="mode">primary</field>
    <field name="inherit_id" ref="ssi_master_data_mixin.mixin_master_data_view_form" />
    <field name="arch" type="xml">
        <data>
            <xpath expr="//field[@name='active']" position="after">
                <field name="tag_id" />
            </xpath>
        </data>
    </field>
</record>

<record id="partner_evaluation_result_action" model="ir.actions.act_window">
    <field name="name">Partner Evaluation Results</field>
    <field name="res_model">partner_evaluation_result</field>
    <field name="view_mode">tree,form</field>
</record>

<menuitem
        id="partner_evaluation_result_menu"
        name="Results"
        parent="menu_partner_evaluation_config"
        action="partner_evaluation_result_action"
        groups="partner_evaluation_result_group"
        sequence="120"
    />
</odoo>
