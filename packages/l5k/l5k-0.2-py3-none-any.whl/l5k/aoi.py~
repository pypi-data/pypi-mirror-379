""" """

import collections
import dataclasses

from . import datatype


@dataclasses.dataclass
class AddOnInstruction:

    attributes: dict
    parameters: collections.OrderedDict
    local_tags: collections.OrderedDict


def convert_parameter(tokens):
    """Converts parsing tokens into a parameter object."""
    param = datatype.Member(
        datatype=tbokens["datatype"],
    )

    return tokens["name"], param
