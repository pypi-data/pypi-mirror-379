<object>
    <hidden manager="plone.belowcontenttitle" skinname="imioapps">
        <viewlet name="imio.history.documentbyline"/>
    </hidden>
    <hidden manager="plone.belowcontenttitle" skinname="*">
        <viewlet name="document-generation-link"/>
    </hidden>
    <hidden manager="plone.belowcontentbody" skinname="imioapps">
        <viewlet name="collective.task.list_tasks"/>
    </hidden>
    <order manager="plone.htmlhead.links" skinname="imioapps"
           based-on="Plone Default">
        <viewlet name="urban.viewlets.urbanfavicon"
                 insert-before="*" />
    </order>
    <hidden manager="plone.portalfooter" skinname="urbanskin">
        <viewlet name="plone.site_actions"/>
    </hidden>
</object>
